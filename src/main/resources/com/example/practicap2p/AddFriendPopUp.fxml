<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.practicap2p.AddFriendPopUpController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../../../../../Downloads/COSA.jpg" />
         </image>
      </ImageView>
      <Text fx:id="TEXTO" fill="#00f000" fontSmoothingType="LCD" layoutX="110.0" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Friend Request" textAlignment="CENTER" wrappingWidth="380.53680419921875">
