            <Font size="34.0" />
         </font>
      </Text>
      <TextField fx:id="FRIENDLABEL" editable="false" layoutX="226.0" layoutY="122.0" />
      <Text fx:id="TEXTO2" fontSmoothingType="LCD" layoutX="138.0" layoutY="193.0" strokeType="OUTSIDE" strokeWidth="0.0" text="WANTS TO BE YOUR FRIEND" wrappingWidth="325.5999389648439">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Button fx:id="Aceptar" layoutX="174.0" layoutY="283.0" mnemonicParsing="false" onAction="#AceptarAmigo" text="ACCEPT" textFill="#09ff1a">
